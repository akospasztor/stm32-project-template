            "includePath": [
                "${commonIncludePath}"
            ],
            "defines": [
                "${commonDefines}"
            ],
            "compilerPath": "/opt/gcc/10-2020-q4/bin/arm-none-eabi-g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-arm"
        },
        {
            "name": "stm32l496-gcc-linux",
            "includePath": [
                "${commonIncludePath}"
            ],
            "defines": [
                "${commonDefines}"
            ],
            "compilerPath": "/opt/gcc/10-2020-q4/bin/arm-none-eabi-g++",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-arm"
