{
    "env": {
        "commonIncludePath": [
            "${workspaceFolder}/lib/CMSIS/Device/ST/STM32L4xx/Include",
            "${workspaceFolder}/lib/CMSIS/Include",
            "${workspaceFolder}/lib/STM32L4xx_HAL_Driver/Inc",
            "${workspaceFolder}/lib/STM32L4xx_HAL_Driver/Inc/Legacy",
            "${workspaceFolder}/mcal/st-stm32l4/include",
            "${workspaceFolder}/include",
